@prefix : <http://localhost:8080/CottageBookingService/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .

# Classes
:Person a owl:Class .
:Cottage a owl:Class .
:City a owl:Class .
:Booking a owl:Class .
:BookingPeriod a owl:Class .
:CottageSuggestion a owl:Class .

# Datatype Properties
:bookerName a owl:DatatypeProperty ;
    rdfs:domain :Booking ;
    rdfs:range xsd:string .

:requiredPlaces a owl:DatatypeProperty ;
    rdfs:domain :Booking ;
    rdfs:range xsd:integer .

:requiredBedrooms a owl:DatatypeProperty ;
    rdfs:domain :Booking ;
    rdfs:range xsd:integer .

:maxDistanceToLake a owl:DatatypeProperty ;
    rdfs:domain :Booking ;
    rdfs:range xsd:integer .

:maxDistanceToCity a owl:DatatypeProperty ;
    rdfs:domain :Booking ;
    rdfs:range xsd:integer .

:requiredDays a owl:DatatypeProperty ;
    rdfs:domain :Booking ;
    rdfs:range xsd:integer .

:startDate a owl:DatatypeProperty ;
    rdfs:domain :BookingPeriod ;
    rdfs:range xsd:date .

:maxShiftDays a owl:DatatypeProperty ;
    rdfs:domain :Booking ;
    rdfs:range xsd:integer .

:bookingNumber a owl:DatatypeProperty ;
    rdfs:domain :Booking ;
    rdfs:range xsd:string .

:cottageAddress a owl:DatatypeProperty ;
    rdfs:domain :Cottage ;
    rdfs:range xsd:string .

:cottageImage a owl:DatatypeProperty ;
    rdfs:domain :Cottage ;
    rdfs:range xsd:anyURI .

:actualPlaces a owl:DatatypeProperty ;
    rdfs:domain :Cottage ;
    rdfs:range xsd:integer .

:actualBedrooms a owl:DatatypeProperty ;
    rdfs:domain :Cottage ;
    rdfs:range xsd:integer .

:actualDistanceToLake a owl:DatatypeProperty ;
    rdfs:domain :Cottage ;
    rdfs:range xsd:integer .

:distanceToCity a owl:DatatypeProperty ;
    rdfs:domain :Cottage ;
    rdfs:range xsd:integer .

:bookingStartDate a owl:DatatypeProperty ;
    rdfs:domain :BookingPeriod ;
    rdfs:range xsd:date .

:bookingEndDate a owl:DatatypeProperty ;
    rdfs:domain :BookingPeriod ;
    rdfs:range xsd:date .

:BookingStatus a owl:DatatypeProperty ;
    rdfs:domain :Booking ;
    rdfs:range xsd:string .

# Object Properties
:nearestCity a owl:ObjectProperty ;
    rdfs:domain :Cottage ;
    rdfs:range :City .

:hasBookingPeriod a owl:ObjectProperty ;
    rdfs:domain :Booking ;
    rdfs:range :BookingPeriod .

:bookedBy a owl:ObjectProperty ;
    rdfs:domain :Booking ;
    rdfs:range :Person .

:booksCottage a owl:ObjectProperty ;
    rdfs:domain :Booking ;
    rdfs:range :Cottage .

:isUnavailable a owl:ObjectProperty ;
    rdfs:domain :Cottage ;
    rdfs:range :BookingPeriod .